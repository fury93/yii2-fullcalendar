{
    "name": "lav45/yii2-fullcalendar",
    "description": "Widget for Yii Framework 2.0 to use FullCalendar",
    "keywords": ["yii2", "extension", "widget", "calendar", "fullcalendar"],
    "homepage": "https://github.com/lav45/yii2-fullcalendar",
    "type": "yii2-extension",
    "license": "BSD 3-Clause",
    "version": "1.1.0",
    "authors": [
        {
            "name": "Alexey Loban",
            "email": "lav451@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.10",
        "yiisoft/yii2-jui": "~2.0.6",
        "bower-asset/moment": "@dev",
        "bower-asset/fullcalendar": "3.*"
    },
    "autoload": {
        "psr-4": {
            "lav45\\widget\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
